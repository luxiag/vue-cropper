{
  "name": "vue-ts-cropper",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "author": "luxiag",
  "license": "MIT",
  "description": "A simple and easy to use vue cropper component",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxiag/vue-cropper.git"
  },
  "keywords": [
    "vue",
    "cropper",
    "image",
    "crop",
    "vue3"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "bugs": "https://github.com/luxiag/vue-cropper/issues",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@types/vue": "^2.0.0",
    "sass-embedded": "^1.80.6",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-plugin-oxlint": "^0.11.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.11.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-tsc": "^2.1.10"
  }
}
